{
  "name": "nimbles-nl/laravel-slack",
  "description": "Laravel package for sending slack messages",
  "keywords": ["laravel", "slack", "package"],
  "homepage": "https://github.com/nimbles-nl/laravel-slack",
  "license": "MIT",
  "authors": [
    {
      "name": "Wessel Strengholt",
      "email": "wessel@nimbles.com"
    }
  ],
  "autoload-dev": {
    "psr-4": { "Tests\\": "tests/" }
  },
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "^5.1",
    "php-http/httplug": "1.1.*",
    "guzzlehttp/psr7": "1.4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "php-http/mock-client": "^1.0"
  },
  "autoload": {
    "psr-0": {
      "Nimbles\\Slack": "src/"
    }
  }
}
